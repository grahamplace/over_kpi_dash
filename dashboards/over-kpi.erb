<% content_for :title do %>Over KPIs<% end %>
<div class="gridster">
  <ul>

  	<li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-view="Image" data-image="/logo.png"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="new_users" data-view="Number" data-title="New Users" data-addclass-danger="isTooHigh" ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="active_users" data-view="Number" data-title="Active Users"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="paying_users" data-view="Number" data-title="Paying Users"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="completed_projects" data-view="Number" data-title="Completed Projects"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="revenue" data-view="Number" data-title="Revenue" data-prefix="$"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="conversion_rate" data-view="Number" data-title="Conversion Rate" data-suffix="%" ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="avg_spent" data-view="Number" data-title="$ Spent/Paying User" data-prefix="$" ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="projects_per_user" data-view="Number" data-title="Completed Projects/User" ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="collects_per_user" data-view="Number" data-title=" Collects/User" ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="new_projects_per_user" data-view="Number" data-title="New Projects/User" ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="stickiness" data-view="Number" data-title="Stickiness" data-suffix="%" ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="3" data-sizey="1">
      <div data-id="n_retention" data-view="Retention" data-title="N-Day Retention" data-suffix="%" ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="3" data-sizey="1">
      <div data-id="u_retention" data-view="Retention" data-title="Unbounded Retention" data-suffix="%" ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-view="Clock"></div>
      <i class="fa fa-clock-o icon-background"></i>
    </li>

  </ul>
</div>
